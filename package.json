{
  "name": "jest-e2e-serverless",
  "version": "0.0.7",
  "description": "Jest matchers for Serverless frameworks",
  "main": "lib/index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "JEST_JUNIT_OUTPUT=./reports/junit/${npm_package_name}-test-results.xml jest --runInBand --ci",
    "coverage": "jest --coverage",
    "prettier": "prettier --write src/**/*.ts",
    "prettier:ci": "prettier --list-different src/**/*.ts",
    "prepublishOnly": "yarn lint && yarn run build"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erezrokah/jest-e2e-serverless.git"
  },
  "homepage": "https://github.com/erezrokah/jest-e2e-serverless/#readme",
  "author": "Erez Rokah",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/jest-diff": "^20.0.0",
    "@types/jest-matcher-utils": "^21.0.1",
    "@types/node": "^10.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "jest-junit": "^5.1.0"
  },
  "peerDependencies": {
    "jest": ">=22.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.323.0",
    "jest-diff": "^23.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
